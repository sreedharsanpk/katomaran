datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GitHubWebhookEvent {
  id         String @id @default(uuid())
  deliveryId String @unique
  eventType  String
  payload    Json
  response   Json

  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}